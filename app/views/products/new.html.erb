<%= form_for [@user, @product] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :size %>
  <%= f.number_field :size %>
  <%= f.submit %>
<% end %>

<%= form_for [@price] do |f| %>
<%= f.label :currency %>
<%= f.select :score, [["Eur"], ["Dollar"]]%>
<%= f.label :value %>
<%= f.number_field :value %>
<%= f.submit %>
<% end %>
